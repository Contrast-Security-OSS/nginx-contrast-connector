    $ngx_addon_dir/dtm.pb-c.c \
    $ngx_addon_dir/settings.pb-c.c \
    $ngx_addon_dir/ngx_http_contrast_connector_module.c"

pre_module='ngx_http_chunked_filter_module'
if [ "$HTTP_V2" = "YES" ]; then
    pre_module='ngx_http_v2_filter_module'
fi

